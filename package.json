{
  "name": "CryptoSeed",
  "version": "1.1.0",
  "description": "Secure your bitcoin recovery seed.",
  "main": "main.js",
  "scripts": {
    "start": "webpack --config webpack-config.js --progress --inline --colors && electron .",
    "watch": "webpack --config webpack-config.js --progress --inline --colors --watch",
    "lint": "eslint --ext .js,.jsx .",
    "pack:linux": "electron-builder build --linux deb --x64",
    "pack:windows": "electron-builder build --win --x64",
    "pack:osx": "electron-builder build --mac zip --x64"
  },
  "repository": "git@github.com:anderson-arlen/cryptoseed.git",
  "keywords": [
    "electron",
    "bitcoin",
    "encryption",
    "bip32",
    "recovery",
    "seed",
    "phrase",
    "words"
  ],
  "author": "Arlen Anderson <anderson.arlen+npm@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "electron": "^11.5.0"
  },
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-regenerator": "^6.21.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "bluebird": "^3.4.7",
    "eslint-plugin-react": "^6.9.0",
    "flexboxgrid": "^6.3.1",
    "material-ui": "^0.16.6",
    "qr-image": "^3.2.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-tap-event-plugin": "^2.0.1",
    "transfer-webpack-plugin": "^0.1.4",
    "webpack": "^1.14.0"
  },
  "build": {}
}
